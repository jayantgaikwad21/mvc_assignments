
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Contact</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/umd/popper.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
</head>
<body>
    <div>
        <nav class="navbar navbar-expand-sm bg-success navbar-dark">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="/home/index">home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/home/Product">Product</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/home/Contact">Contact</a>
                </li>
            </ul>
        </nav>
        @{
            string StudentResult;
            if (ViewBag.Marks >= 35)
            {
                StudentResult = "pass";
            }
            else
            {
                StudentResult = "fail";
            }
        }
        <div class="container-fluid">
            <table class="table">
                <tr>
                    <td>StudentId</td>
                    <td>@ViewBag.StudentId</td>
                </tr>
                <tr>
                    <td>StudentName</td>
                    <td>@ViewBag.StudentName</td>
                </tr>
                <tr>
                    <td>Marks</td>
                    <td>@ViewBag.Marks</td>
                </tr>
                <tr>
                    <td>Result</td>
                    <td>@StudentResult</td>
                </tr>

                <tr>
                    <td>subject</td>
                    <td>
                        @for (int i = 1; i < ViewBag.NoOfSemister; i++)
                        {
                            <span>@i</span>
                        }
                    </td>
                </tr>
                <tr>
                    <td>Subjects</td>
                    <td>
                        @foreach (var subject in ViewBag.Subject)
                        {
                            <span>@subject</span>
                        }
                    </td>
                </tr>
            </table>
</body>
</html>
